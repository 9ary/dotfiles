# Fix for auto completion
alias sudo="sudo "

alias ls="ls --color=auto -Fh"
alias lsa="ls --color=auto -FAh"
alias ll="ls --color=auto -Fhl"
alias lla="ls --color=auto -FAhl"
alias cp="cp -r"
alias rsync="rsync -rPh"
alias v="nvim"
alias rg="rg -S"
alias pgrep="pgrep -l"

alias verynice="schedtool -B -n 15 -e ionice -c3 "

