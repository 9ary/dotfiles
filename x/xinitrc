#!/usr/bin/bash

source ~/.envrc

xrdb -merge -override ~/dotfiles/x/Xresources

# Don't ring bells
xset b off

# Mouse settings
razer-profile

pulseaudio --start
autocutsel -fork &
dunst &
chromium &
ytdl_chromium.py &
[[ -z $(pgrep transmission-da) ]] && transmission-daemon
thunderbird &
telegram &
$TERMINAL -title "WeeChat" -e weechat &
dropbox &
nm-applet &
redshift-gtk &

~/.fehbg &
compton &
exec i3

