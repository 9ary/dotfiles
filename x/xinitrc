#!/usr/bin/zsh

xrdb ~/dotfiles/x/Xresources

# Don't ring bells
xset b off

# Mouse settings
razer-profile

xsettingsd &
pulseaudio --start
autocutsel -fork &
dunst &
keynav &
chromium &
ytdl_server.py &
killall -9 qbittorrent; qbittorrent &
thunderbird &
telegram &
$TERMINAL -title "WeeChat" -e weechat &
dropbox &
nm-applet &
udiskie &
redshift-gtk &

~/.fehbg &
compton &
exec i3

